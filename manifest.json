{
 "manifest_version":2,

 "name":"LetsGoShip",

 "version":"1",

 "browser_action":{
 	"default_icon":"/images/logo.png",
 	"default_title":"A popup will come here."
 },

 "permissions": [
    "activeTab", "http://ex.travelcast.us/api/*", "webRequest", "webRequestBlocking" , "storage"
 ],

 "content_scripts": [
  {
	"matches": [
      "https://www.amazon.com/*",
      "https://www.nike.com/*",
      "https://www.ebay.com/*"
    ],
	"js": ["/jquery-3.4.0.min.js", "/js/generate.js","/js/content.js"]
  }],

 "background":{
 	"scripts":["/jquery-3.4.0.min.js"]
 },

 "web_accessible_resources": [
    "/html/topbar.html",
    "/images/topbarLogo.png",
    "/images/cartIcon.png",
    "/images/success.png",
    "/images/amazon.png",
    "/images/nike.png",
    "/images/ebay.png"
  ],

 "content_security_policy":  "script-src 'self' https://code.jquery.com/jquery-3.3.1.slim.min.js https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js; object-src 'self'"
}


